# Function to calculate the minimum number of moves
def min_moves_to_reach_stair(x, y):
    return (x + y - 1) // y

# Input the number of test cases
t = int(input())

# Iterate through each test case
for _ in range(t):
    x, y = map(int, input().split())
    # Calculate and print the minimum number of moves
    print(min_moves_to_reach_stair(x, y))
