# Function to calculate the minimum number of button presses
def min_button_presses(X, Y):
    return abs(X - Y)

# Read the number of test cases
T = int(input())

# Iterate through each test case
for _ in range(T):
    X, Y = map(int, input().split())
    result = min_button_presses(X, Y)
    print(result)
