# Input the number of test cases
T = int(input())

# Iterate through each test case
for _ in range(T):
    # Input N and M for the current test case
    N, M = map(int, input().split())
    
    # Calculate the maximum possible runs in remaining overs
    max_runs_possible = M * 6
    
    # Check if Chef's team can win
    if N <= max_runs_possible:
        print("YES")
    else:
        print("NO")
